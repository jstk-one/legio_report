    <script>
        // 관리자가 설정하는 구글 스프레드시트 URL
        const GOOGLE_SCRIPT_URL = 'https://script.google.com/macros/s/AKfycbwdSvoaDFxxmYtzGTv4cO-0q_pFAYz8ZfZaXHR_0nZ7q5JsO-Q4R31peW5Ac_7TnaOV/exec';

        // 페이지 로드 시 초기화
        document.addEventListener('DOMContentLoaded', function() {
            // 현재 날짜 설정
            const today = new Date();
            document.getElementById('report_date').valueAsDate = today;
            
            // 자동 계산 이벤트 리스너
            setupAutoCalculations();
            
            // 초기 합계 계산
            calculateAllTotals();
        });

        // 자동 계산 설정
        function setupAutoCalculations() {
            // 단원 현황 - 전월/금월 변경 시 자동 계산
            const memberFields = ['active_m', 'active_f', 'active_total', 'pret', 'coop_m', 'coop_f', 'coop_total', 'adj'];
            memberFields.forEach(field => {
                ['prev', 'curr'].forEach(prefix => {
                    const el = document.getElementById(`${prefix}_${field}`);
                    if (el && !el.readOnly) {
                        el.addEventListener('input', calculateMemberStats);
                    }
                });
            });

            // 행동단원 남/여 합계
            ['prev', 'curr'].forEach(prefix => {
                document.getElementById(`${prefix}_active_m`).addEventListener('input', function() {
                    const m = parseInt(this.value) || 0;
                    const f = parseInt(document.getElementById(`${prefix}_active_f`).value) || 0;
                    document.getElementById(`${prefix}_active_total`).value = m + f;
                    calculateMemberStats();
                });
                document.getElementById(`${prefix}_active_f`).addEventListener('input', function() {
                    const m = parseInt(document.getElementById(`${prefix}_active_m`).value) || 0;
                    const f = parseInt(this.value) || 0;
                    document.getElementById(`${prefix}_active_total`).value = m + f;
                    calculateMemberStats();
                });
            });

            // 협조단원 남/여 합계
            ['prev', 'curr'].forEach(prefix => {
                document.getElementById(`${prefix}_coop_m`).addEventListener('input', function() {
                    const m = parseInt(this.value) || 0;
                    const f = parseInt(document.getElementById(`${prefix}_coop_f`).value) || 0;
                    document.getElementById(`${prefix}_coop_total`).value = m + f;
                    calculateMemberStats();
                });
                document.getElementById(`${prefix}_coop_f`).addEventListener('input', function() {
                    const m = parseInt(document.getElementById(`${prefix}_coop_m`).value) || 0;
                    const f = parseInt(this.value) || 0;
                    document.getElementById(`${prefix}_coop_total`).value = m + f;
                    calculateMemberStats();
                });
            });

            // 활동 항목 변경 시 카테고리 합계 계산
            for (let cat = 1; cat <= 12; cat++) {
                const itemCount = getItemCountForCategory(cat);
                for (let i = 1; i <= itemCount; i++) {
                    const el = document.getElementById(`cat${cat}_${i}`);
                    if (el) {
                        el.addEventListener('input', () => calculateCategoryTotal(cat));
                    }
                }
            }

            // 회계보고 자동 계산
            ['finance_carryover', 'finance_membership', 'finance_donation', 'finance_sales', 'finance_other_income', 'finance_expense'].forEach(id => {
                document.getElementById(id).addEventListener('input', calculateFinance);
            });
        }

        // 카테고리별 항목 개수
        function getItemCountForCategory(cat) {
            const counts = {
                1: 17, 2: 12, 3: 3, 4: 3, 5: 3, 6: 3,
                7: 5, 8: 4, 9: 5, 10: 4, 11: 7, 12: 7
            };
            return counts[cat] || 0;
        }

        // 단원 현황 증감 계산
        function calculateMemberStats() {
            const fields = ['active_m', 'active_f', 'active_total', 'pret', 'coop_m', 'coop_f', 'coop_total', 'adj'];
            
            fields.forEach(field => {
                const prev = parseInt(document.getElementById(`prev_${field}`).value) || 0;
                const curr = parseInt(document.getElementById(`curr_${field}`).value) || 0;
                const diff = curr - prev;
                
                // 증가/감소 분리
                if (diff > 0) {
                    document.getElementById(`inc_${field}`).value = diff;
                    document.getElementById(`dec_${field}`).value = 0;
                } else if (diff < 0) {
                    document.getElementById(`inc_${field}`).value = 0;
                    document.getElementById(`dec_${field}`).value = Math.abs(diff);
                } else {
                    document.getElementById(`inc_${field}`).value = 0;
                    document.getElementById(`dec_${field}`).value = 0;
                }
            });
        }

        // 카테고리 합계 계산
        function calculateCategoryTotal(cat) {
            const itemCount = getItemCountForCategory(cat);
            let total = 0;
            
            for (let i = 1; i <= itemCount; i++) {
                const val = parseInt(document.getElementById(`cat${cat}_${i}`).value) || 0;
                total += val;
            }
            
            document.getElementById(`total_cat${cat}`).textContent = total;
        }

        // 모든 카테고리 합계 계산
        function calculateAllTotals() {
            for (let cat = 1; cat <= 12; cat++) {
                calculateCategoryTotal(cat);
            }
            calculateFinance();
        }

        // 회계 계산
        function calculateFinance() {
            const carryover = parseInt(document.getElementById('finance_carryover').value) || 0;
            const membership = parseInt(document.getElementById('finance_membership').value) || 0;
            const donation = parseInt(document.getElementById('finance_donation').value) || 0;
            const sales = parseInt(document.getElementById('finance_sales').value) || 0;
            const otherIncome = parseInt(document.getElementById('finance_other_income').value) || 0;
            const expense = parseInt(document.getElementById('finance_expense').value) || 0;
            
            const totalIncome = carryover + membership + donation + sales + otherIncome;
            const nextCarryover = totalIncome - expense;
            
            document.getElementById('finance_total_income').value = totalIncome;
            document.getElementById('finance_next_carryover').value = nextCarryover;
        }

        // 알림 표시
        function showAlert(message, type) {
            const alert = document.getElementById('alert');
            alert.textContent = message;
            alert.className = `alert alert-${type}`;
            alert.style.display = 'block';
            
            setTimeout(() => {
                alert.style.display = 'none';
            }, 5000);
        }

        // 데이터 수집
        function collectFormData() {
            const data = {
                // 기간 정보
                report_year: document.getElementById('report_year').value,
                report_month: document.getElementById('report_month').value,
                report_start_seq: document.getElementById('report_start_seq').value,
                report_end_seq: document.getElementById('report_end_seq').value,
                
                // 작성자 정보
                presidium_name: document.getElementById('presidium_name').value,
                writer_leader: document.getElementById('writer_leader').value,
                
                // 1-3번
                meetingDay: document.getElementById('meetingDay').value,
                meetingTime: document.getElementById('meetingTime').value,
                location: document.getElementById('location').value,
                attendance_officers: document.getElementById('attendance_officers').value,
                attendance_members: document.getElementById('attendance_members').value,
                
                // 4. 간부명단
                leader_name: document.getElementById('leader_name').value,
