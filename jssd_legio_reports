        }

        // 폼 유효성 검사
        function validateForm() {
            const required = [
                { id: 'report_year', name: '보고 연도' },
                { id: 'report_month', name: '작성 대상 월' },
                { id: 'presidium_name', name: '쁘레시디움명' },
                { id: 'writer_leader', name: '단장' }
            ];
            
            for (const field of required) {
                const value = document.getElementById(field.id).value.trim();
                if (!value) {
                    showAlert(`${field.name}을(를) 입력해주세요.`, 'error');
                    document.getElementById(field.id).focus();
                    return false;
                }
            }
            
            return true;
        }

        // 보고서 제출
        async function submitReport() {
            if (!validateForm()) {
                return;
            }
            
            if (!GOOGLE_SCRIPT_URL || GOOGLE_SCRIPT_URL === 'YOUR_GOOGLE_APPS_SCRIPT_URL_HERE') {
                showAlert('관리자에게 문의하세요: 구글 스프레드시트 URL이 설정되지 않았습니다.', 'error');
                return;
            }
            
            const data = collectFormData();
            
            // 로딩 표시
            document.getElementById('loading').style.display = 'block';
            
            try {
                const response = await fetch(GOOGLE_SCRIPT_URL, {
                    method: 'POST',
                    mode: 'no-cors',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(data)
                });
                
                // no-cors 모드에서는 응답을 읽을 수 없으므로 성공으로 간주
                showAlert('보고서가 성공적으로 제출되었습니다!', 'success');
                
            } catch (error) {
                console.error('제출 오류:', error);
                showAlert('제출 중 오류가 발생했습니다. 다시 시도해주세요.', 'error');
            } finally {
                document.getElementById('loading').style.display = 'none';
            }
        }

        // 출력 기능
        function printReport() {
            window.print();
        }
    </script>
</body>
</html>
