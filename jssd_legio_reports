                event4_host: document.getElementById('event4_host').value,
                event4_date: document.getElementById('event4_date').value,
                event4_location: document.getElementById('event4_location').value,
                event4_attendance: document.getElementById('event4_attendance').value,
                
                event5_title: document.getElementById('event5_title').value,
                event5_host: document.getElementById('event5_host').value,
