                treasurer_baptism: document.getElementById('treasurer_baptism').value,
                treasurer_date: document.getElementById('treasurer_date').value,
                
                // 5. 단원현황
                prev_active_m: document.getElementById('prev_active_m').value,
                prev_active_f: document.getElementById('prev_active_f').value,
                prev_active_total: document.getElementById('prev_active_total').value,
                prev_pret: document.getElementById('prev_pret').value,
                prev_coop_m: document.getElementById('prev_coop_m').value,
                prev_coop_f: document.getElementById('prev_coop_f').value,
                prev_coop_total: document.getElementById('prev_coop_total').value,
                prev_adj: document.getElementById('prev_adj').value,
                
                curr_active_m: document.getElementById('curr_active_m').value,
                curr_active_f: document.getElementById('curr_active_f').value,
                curr_active_total: document.getElementById('curr_active_total').value,
                curr_pret: document.getElementById('curr_pret').value,
                curr_coop_m: document.getElementById('curr_coop_m').value,
                curr_coop_f: document.getElementById('curr_coop_f').value,
                curr_coop_total: document.getElementById('curr_coop_total').value,
                curr_adj: document.getElementById('curr_adj').value,
                
                inc_active_m: document.getElementById('inc_active_m').value,
                inc_active_f: document.getElementById('inc_active_f').value,
                inc_active_total: document.getElementById('inc_active_total').value,
                inc_pret: document.getElementById('inc_pret').value,
                inc_coop_m: document.getElementById('inc_coop_m').value,
                inc_coop_f: document.getElementById('inc_coop_f').value,
                inc_coop_total: document.getElementById('inc_coop_total').value,
                inc_adj: document.getElementById('inc_adj').value,
                
                dec_active_m: document.getElementById('dec_active_m').value,
                dec_active_f: document.getElementById('dec_active_f').value,
                dec_active_total: document.getElementById('dec_active_total').value,
                dec_pret: document.getElementById('dec_pret').value,
                dec_coop_m: document.getElementById('dec_coop_m').value,
                dec_coop_f: document.getElementById('dec_coop_f').value,
                dec_coop_total: document.getElementById('dec_coop_total').value,
                dec_adj: document.getElementById('dec_adj').value,
                
                // 6. 주요사항
                event1_title: document.getElementById('event1_title').value,
                event1_host: document.getElementById('event1_host').value,
                event1_date: document.getElementById('event1_date').value,
                event1_location: document.getElementById('event1_location').value,
                event1_attendance: document.getElementById('event1_attendance').value,
                
                event2_title: document.getElementById('event2_title').value,
                event2_host: document.getElementById('event2_host').value,
                event2_date: document.getElementById('event2_date').value,
                event2_location: document.getElementById('event2_location').value,
                event2_attendance: document.getElementById('event2_attendance').value,
                
                event3_title: document.getElementById('event3_title').value,
                event3_host: document.getElementById('event3_host').value,
                event3_date: document.getElementById('event3_date').value,
                event3_location: document.getElementById('event3_location').value,
                event3_attendance: document.getElementById('event3_attendance').value,
                
